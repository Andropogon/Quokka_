}



fun floor(num) {
    yield(to_int(num))
}

fun ceil(num) {
    inteiro = to_int(num)
    if(num > to_float(inteiro)) {
        yield(inteiro + 1)
    }
    yield(inteiro)
}

fun sqrt(num) {
    yield(num ** 0.5)
}

fun pow(base, exp) {
    yield(base ** exp)
}

fun factorial(n) {
    if(n <= 1) {
        yield(1)
    }
    resultado = 1
    i = 2
    while(i <= n) {
        resultado = resultado * i
        i++
    }
    yield(resultado)
}

fun avg(array) {
    total = sum(array)
    quantidade = len(array)
    yield(total / quantidade)
}
}