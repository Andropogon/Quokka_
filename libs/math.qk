main{
fun abs(num) {
    if(num < 0) {
        yield(num * -1)
    }
    yield(num)
}

fun round(num) {
    if(num >= 0) {
        yield(to_int(num + 0.5)
    } else {
        yield(to_int(num - 0.5))
    }
}

while(contador < 10){
